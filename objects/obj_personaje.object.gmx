if (vsp &lt; 10) {vsp += grav;}
if (place_meeting(x, y+1,obj_muro2)){
        vsp = key_up * -jumpspeed;
} 

// Colision vertical
if (place_meeting(x, y+vsp,obj_muro2)){
     while(!place_meeting(x, y+sign(vsp), obj_muro2)){
     y += sign(vsp)
     }
     vsp = 0;
}
y += vsp;
              
