hsp = move * movespeed;

// Colision horizontal 
if (place_meeting(x+hsp,y,obj_muro2)){
     while(!place_meeting(x+sign(hsp), y, obj_muro2)){
     x += sign(hsp)
     }
     hsp = 0;
}
